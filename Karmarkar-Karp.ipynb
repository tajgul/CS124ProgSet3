   "source": [
    "def kk(A):\n",
    "    h = [-i for i in A]  \n",
    "    heapq.heapify(h)\n",
    "    while len(h) > 1:\n",
    "        heapq.heappush(h, -((-heapq.heappop(h))  - (-heapq.heappop(h))) )\n",
    "    return -h[0] "
   ]
  },
